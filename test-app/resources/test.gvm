######### START

jump start # <- goto entry point

test-fn:
    frame 0 1
    push 33
    store 0
    push "OK"
    load 0
    call dump
    return

dump:
    frame 1 0
    print
    push " "
    print
    return

addfn:
    frame 2 0
    add
    push "hello"
    call dump
    return

start:
    push 2
    push 20
    call addfn
    call test-fn
    return

######### END